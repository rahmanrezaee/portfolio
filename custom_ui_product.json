{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "Images"
          }
        }
      }
    },
    "body": {
      "type": "list_view",
      "args": {
        "children": [
          {
              "type": "container",
              "args": {
                "width": "240",
                "child": {
                  "type": "card",
                  "args": {
                    "color": "#E91E63",
                    "elevation": 10,
                    "shape": {
                      "type": "rounded",
                      "borderRadius": 15
                    },
                    "child": {
                      "type": "column",
                      "args": {
                        "mainAxisSize": "min",
                        "children": [
                          {
                            "type": "list_tile",
                            "args": {
                              "leading": {
                                "type": "icon",
                                "args": {
                                  "icon": {
                                    "codePoint": 57369,
                                    "fontFamily": "MaterialIcons"
                                  },
                                  "size": 70
                                }
                              },
                              "title": {
                                "type": "text",
                                "args": {
                                  "text": "Heart Shaker",
                                  "style": {
                                    "color": "#fff"
                                  }
                                }
                              },
                              "subtitle": {
                                "type": "text",
                                "args": {
                                  "text": "TWICE",
                                  "style": {
                                    "color": "#fff"
                                  }
                                }
                              }
                            }
                          },
                          {
                            "type": "button_bar",
                            "args": {
                              "children": [
                                {
                                  "type": "elevated_button",
                                  "args": {
                                    "onPressed": "${noop()}",
                                    "child": {
                                      "type": "text",
                                      "args": {
                                        "text": "Edit",
                                        "style": {
                                          "color": "#fff"
                                        }
                                      }
                                    }
                                  }
                                },
                                {
                                  "type": "elevated_button",
                                  "args": {
                                    "onPressed": "${noop()}",
                                    "child": {
                                      "type": "text",
                                      "args": {
                                        "text": "Delete",
                                        "style": {
                                          "color": "#fff"
                                        }
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    }
                  }
                }
              }
            },
           {
            "type": "text",
            "args": {
              "text": "Hello World"
            }
          },
          {
            "type": "text",
            "args": {
              "text": "Network"
            }
          },
          {
            "type": "network_image",
            "args": {
              "fit": "fitWidth",
              "src": "https://upload.wikimedia.org/wikipedia/commons/thumb/5/51/Google.png/320px-Google.png"
            }
          }
        ]
      }
    }
  }
}
