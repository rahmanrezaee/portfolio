{
  "type": "container",
  "args": {
    "width": 240,
    "child": {
      "type": "card",
      "args": {
        "color": "#AAAA11",
        "elevation": 10,
        "shape": {
          "type": "rounded",
          "borderRadius": 50
        },
        "child": {
          "type": "column",
          "args": {
            "mainAxisSize": "min",
            "children": [
              {
                "type": "text",
                "args": {
                  "text": "Network Second time"
                }
              },
              {
                "type": "network_image",
                "args": {
                  "fit": "fitWidth",
                  "src": "https://upload.wikimedia.org/wikipedia/commons/thumb/5/51/Google.png/320px-Google.png"
                }
              }
            ]
          }
        }
      }
    }
  }
}
